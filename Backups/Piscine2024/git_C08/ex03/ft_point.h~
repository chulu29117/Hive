#ifndef FT_POINT_H
# define FT_POINT_H

// Structure definition
typedef struct s_point		// Defines a new data type s_point, structure containing x and y
{
	int	x;
	int	y;
}	t_point;		// Alias of this structure

void	set_point(t_point *point);	// Takes a pointer to t_point structure

#endif
